        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="insert">
          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import com.mongodb.MongoClientURI
import com.mongodb.MongoClient
import com.mongodb.client.MongoDatabase
import com.mongodb.client.MongoCollection      
import org.bson.Document


MongoClient mongoClient = vars.getObject(&quot;mongoClient&quot;)


// Select Database and Collection
MongoDatabase database = mongoClient.getDatabase(&quot;edm&quot;)
MongoCollection&lt;Document&gt; collection = database.getCollection(&quot;documentPayload&quot;)

List&lt;Document&gt; documents = new ArrayList&lt;&gt;()

for(int i=0 ; i&lt;500;i++){
// Create a new document
Document newDoc = new Document()
        .append(&quot;documentId&quot;, &quot;${__Random(10000000000000000,99999999999999999)}&quot;)
        .append(&quot;documentCode&quot;, &quot;${__UUID()}&quot;)
        .append(&quot;documentName&quot;, &quot;سند مالکیت ملک&quot;)
        .append(&quot;description&quot;, &quot;توضیحات&quot;)
        .append(&quot;pageCount&quot;, 1)
);

        
        documents.add(newDoc)

}


// Insert the document into the collection
collection.insertMany(documents)
//log.info(&quot;Inserted document: ${newDoc}&quot;)

// Perform a Find Operation 
//Document firstDoc = collection.find().first()
//log.info(&quot;First document: ${firstDoc}&quot;)

// Close connection
//mongoClient.close()</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
